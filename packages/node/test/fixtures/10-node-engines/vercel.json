{
  "version": 2,
  "builds": [
    {
      "src": "**/*.js",
      "use": "@vercel/node"
    }
  ],
  "probes": [
    { "path": "/empty", "mustContain": "RANDOMNESS_PLACEHOLDER:empty:14" },
    { "path": "/greater", "mustContain": "RANDOMNESS_PLACEHOLDER:greater:14" },
    { "path": "/major", "mustContain": "RANDOMNESS_PLACEHOLDER:major:12" },
    { "path": "/range", "mustContain": "RANDOMNESS_PLACEHOLDER:range:12" }
  ]
}
